{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "spread": true,
    "restParams": true,
    "experimentalObjectRestSpread": true
  },
  "rules": {
    "strict": [0],
    "no-param-reassign": [0],
    "react/jsx-closing-bracket-location": [0, "line-aligned"]
  }
}
