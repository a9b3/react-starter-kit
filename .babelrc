{
  "presets": ["es2015-webpack", "react", "stage-0"],
  "env": {
    "development": {
      "plugins": [
        [
          "react-transform",
          {
            "transforms":
              [
                {
                  "transform": "react-transform-hmr",
                  "imports": ["react"],
                  "locals": ["module"]
                },
                {
                  "transform": "react-transform-catch-errors",
                  "imports": [
                    "react",
                    "redbox-react"
                  ]
                }
              ]
          }
        ]
      ]
    }
  },
  "plugins": [
    "transform-flow-strip-types"
  ]
}
