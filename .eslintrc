{
  "parser": "babel-eslint",
    "env": {
      "es6": true,
      "node": true,
      "browser": true,
    },
    "rules": {
      "no-debugger": 2,
      "no-cond-assign": 2,
      "no-constant-condition": 2,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-empty-character-class": 2,
      "no-empty": 2,
      "no-extra-boolean-cast": 2,
      "no-extra-semi": 2,
      "no-func-assign": 2,
      "no-inner-declarations": 2,
      "no-invalid-regexp": 2,
      "no-irregular-whitespace": 2,
      "no-unreachable": 2,
      "valid-jsdoc": 2,
      "valid-typeof": 2,
      "dot-notation": 2
    }
}
